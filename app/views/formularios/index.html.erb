#encoding: utf-8
<% provide(:title, 'Formularios de Inscripcion del usuario') %>
<h1>Formularios</h1>

<table>
  <tr>

  	<th></th>
    <th>Fecha</th>
    <th></th>
    <th></th>
    <th></th>
    <th>Grupo</th>
    <th></th>
    <th></th>
    <th></th>
    <th>Estado</th>
    <th></th>
  </tr>

  <%= content_tag_for(:tr, @formulario) do |formulario| %>

    <td><%= formulario.created_at %></td>
    <td><%= formulario.principal.nombre %></td>
    <td><%= formulario.estado %></td>

    <td><%= link_to t("scaffoldi18n.edit"), edit_formulario_path(formulario) %></td>
    
  <% end %>
</table>

<br />

	<%= link_to t('scaffoldi18n.new'), formularios_path, method: :post %>
